    <panel>
      <chart>
        <search>
          <query>| inputlookup app_usage.csv
| table _time OTHER Recruiting
| fit MLTKContainer algo=random_cut_forest OTHER from Recruiting threshold=0.1
| rename predicted_OTHER as outlier
| eval collusive_displacement=collusive_displacement/30
| table _time *</query>
          <earliest>-60m@m</earliest>
          <latest>now</latest>
          <sampleRatio>1</sampleRatio>
        </search>
