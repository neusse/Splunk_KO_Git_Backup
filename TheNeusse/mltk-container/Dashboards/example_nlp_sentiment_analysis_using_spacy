    <panel>
      <single>
        <title>Average Polarity</title>
        <search base="base">
          <query>|stats avg(sentiment_polarity)</query>
        </search>
        <option name="numberPrecision">0.00</option>
        <option name="rangeColors">["0x53a051","0x0877a6","0xf8be34","0xf1813f","0xdc4e41"]</option>
      </single>
    </panel>
    <panel>
      <single>
        <title>Average Subjectivity</title>
        <search base="base">
          <query>|stats avg(sentiment_subjectivity)</query>
        </search>
        <option name="numberPrecision">0.00</option>
        <option name="rangeColors">["0x53a051","0x0877a6","0xf8be34","0xf1813f","0xdc4e41"]</option>
      </single>
    </panel>
  </row>
  <row>
    <panel>
      <chart>
        <title>Polarity and Subjectivity by Key Terms</title>
        <search base="base">
          <query>| table sentiment_assessments
| eval sentiment_assessments="[".trim(sentiment_assessments,"[]()")
| makemv delim="), (" sentiment_assessments
| mvexpand sentiment_assessments
| rex field=sentiment_assessments "\[(?&lt;terms&gt;.*)\],(?&lt;polarity&gt;.*),(?&lt;subjectivity&gt;.*),.*"
| table terms polarity subjectivity
| makemv delim="," terms
| eval terms=trim(terms,"'"), polarity=tonumber(trim(polarity)), subjectivity=tonumber(trim(subjectivity))
| where polarity!=0 AND subjectivity!=0
| mvexpand terms
| stats max(polarity) as polarity max(subjectivity) as subjectivity by terms
| sort - polarity</query>
        </search>
        <option name="charting.chart">column</option>
        <option name="charting.drilldown">all</option>
        <option name="charting.legend.placement">bottom</option>
        <option name="refresh.display">progressbar</option>
      </chart>
    </panel>
  </row>
  <row>
    <panel>
      <table>
        <title>Raw Data</title>
        <search id="base">
          <query>| makeresults
| eval text = "$text$"
| makemv text delim=";"
| mvexpand text
| fit MLTKContainer algo=spacy_sentiment epochs=100 text into app:spacy_sentiment_model as sentiment</query>
          <earliest>-24h@h</earliest>
          <latest>now</latest>
        </search>
        <option name="drilldown">none</option>
        <format type="color" field="sentiment_polarity">
          <colorPalette type="minMidMax" maxColor="#4FA484" midColor="#FFFFFF" minColor="#F8BE34"></colorPalette>
          <scale type="minMidMax" midType="percentile" midValue="50"></scale>
        </format>
        <format type="color" field="sentiment_subjectivity">
          <colorPalette type="minMidMax" maxColor="#006D9C" minColor="#FFFFFF"></colorPalette>
          <scale type="minMidMax"></scale>
        </format>
      </table>
    </panel>
  </row>
