    <input type="dropdown" token="myComparisonSpan" searchWhenChanged="true">
      <label>Time Range Comparison</label>
      <choice value="prev2Days">Previous Two Days</choice>
      <choice value="prev2Weeks">Previous Two Weeks</choice>
      <choice value="lastVsCurrentWeek">Last Week vs. This Week</choice>
