    <panel>
      <table>
        <search>
          <query>| makeresults
| eval mySpanID="$myComparisonSpan$"
| eval myEarliest=case(mySpanID == "prev2Days", relative_time(now(),"-2d@d"), mySpanID == "prev2Weeks", relative_time(now(), "-2w@w0"), mySpanID == "lastVsCurrentWeek", relative_time(now(), "-1w@w0"), mySpanID == "lastVsCurrentMonth", relative_time(now(), "-1mon@mon"), mySpanID == "prev2Months", relative_time(now(), "-2mon@mon"), mySpanID == "last90Vs180", relative_time(now(), "-180d@d"), 1=1, 0)
| eval myLatest=case(mySpanID == "prev2Days", relative_time(now(),"@d"), mySpanID == "prev2Weeks", relative_time(now(), "@w0"), mySpanID == "lastVsCurrentWeek", relative_time(now(), "@d"), mySpanID == "lastVsCurrentMonth", relative_time(now(), "@d"), mySpanID == "prev2Months", relative_time(now(), "@mon"), mySpanID == "last90Vs180", relative_time(now(), "@d"), 1=1, now())
| eval mySplitTime=case(mySpanID == "prev2Days", relative_time(now(), "-1d@d"), mySpanID == "prev2Weeks", relative_time(now(), "-1w@w0"), mySpanID == "lastVsCurrentWeek", relative_time(now(), "@w0"), mySpanID == "lastVsCurrentMonth", relative_time(now(), "@mon"), mySpanID == "prev2Months", relative_time(now(), "-1mon@mon"), mySpanID == "last90Vs180", relative_time(now(), "-90d@d"), 1=1, 0)
| eval myPrevName=case(mySpanID == "prev2Days", "Two Days Ago", mySpanID == "prev2Weeks", "Two Weeks Ago", mySpanID == "lastVsCurrentWeek", "Last Week", mySpanID == "lastVsCurrentMonth", "Last Month", mySpanID == "prev2Months", "Two Months Ago", mySpanID == "last90Vs180", "180 - 90 Days Ago", 1=1, "Previous")
| eval myLastName=case(mySpanID == "prev2Days", "Yesterday", mySpanID == "prev2Weeks", "Last Week", mySpanID == "lastVsCurrentWeek", "This Week", mySpanID == "lastVsCurrentMonth", "This Month", mySpanID == "prev2Months", "Last Month", mySpanID == "last90Vs180", "90 - 0 Days Ago", 1=1, "Last")
| eval mySpanUnit=case(mySpanID == "lastVsCurrentMonth", "mon", mySpanID == "prev2Months", "mon", mySpanID == "last90Vs180", "mon", 1=1, "d")</query>
          <earliest>-60m@m</earliest>
          <latest>now</latest>
          <finalized>
            <set token="myEarliest">$result.myEarliest$</set>
            <set token="myLatest">$result.myLatest$</set>
            <set token="mySplitTime">$result.mySplitTime$</set>
            <set token="myPrevName">$result.myPrevName$</set>
            <set token="myLastName">$result.myLastName$</set>
            <set token="mySpanUnit">$result.mySpanUnit$</set>
          </finalized>
        </search>
