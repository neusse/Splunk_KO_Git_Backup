  <row>
    <panel>
      <title>Dashboards created within $tok_app_age_in_days$ days</title>
      <table>
        <search>
          <query>`cst_summary_index` AND source=cst:dashboard:metadata
|rename title as dashboard,  eai:acl.app as app
|eval title = app
|search app = $tok_app_title$
|stats max(_time) as latest min(_time) as earliest by app dashboard author
|eval dashboard_age_in_days = ceil((latest-earliest)/60/60/24), installed_on = strftime(earliest,"%Y-%m-%d")
|search dashboard_age_in_days &lt; $tok_app_age_in_days$
|table app dashboard author installed_on dashboard_age_in_days</query>
          <earliest>$tok_time.earliest$</earliest>
          <latest>$tok_time.latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="count">100</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">none</option>
        <option name="percentagesRow">false</option>
        <option name="refresh.display">progressbar</option>
        <option name="rowNumbers">false</option>
        <option name="totalsRow">false</option>
        <option name="wrap">true</option>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <title>Dashboards not accessed over $tok_app_age_in_days$ days</title>
      <table>
        <search>
          <query>`cst_summary_index` source=cst:dashboard:access
|eval title=app
|search app = $tok_app_title$
|stats max(_time) as last_accessed by app dashboard
|eval now=now(), days_since_last_accessed = ceil((now-last_accessed)/60/60/24), last_accessed=strftime(last_accessed,"%Y-%m-%d")
|search days_since_last_accessed &gt; $tok_app_age_in_days$
|fields app dashboard last_accessed</query>
          <earliest>$tok_time.earliest$</earliest>
          <latest>$tok_time.latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="count">100</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">none</option>
        <option name="percentagesRow">false</option>
        <option name="refresh.display">progressbar</option>
        <option name="rowNumbers">false</option>
        <option name="totalsRow">false</option>
        <option name="wrap">true</option>
      </table>
    </panel>
  </row>

  <row>
    <panel>
      <title>Dashboards Changed in the last 48 hours</title>
      <table>
        <search>
          <query>`cst_summary_index` AND source=cst:dashboard:metadata
|rename title as dashboard,  eai:acl.app as app, eai:data as xml
|eval title = app
|eval search_hash=md5(xml)
|stats dc(search_hash) as versions by app dashboard eai:acl.owner
|search versions &gt; 1
|rename eai:acl.owner as owner
|fields app dashboard owner versions</query>
          <earliest>$tok_time.earliest$</earliest>
          <latest>$tok_time.latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="count">100</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">cell</option>
        <option name="percentagesRow">false</option>
