index=_internal sourcetype=splunk_web_service component=startup build!=000 earliest=-4h@h latest=now
| fields _time host version build 
| eval Splunk_Instance = trim(lower(host)) 
| `get_cluster_label(Splunk_Instance)` 
| stats VALUES(Splunk_Roles) AS Splunk_Roles VALUES(Splunk_Instance) as Splunk_Instance LATEST(version) as version LATEST(build) as build LATEST(_time) as _time BY cluster_label 
