	"definition": 
eval et_diff = case ( exec_time>search_et, (exec_time-search_et)/60, true(), (search_lt-search_et)/60) 
| eval et_range = case (
    et_diff <= 0, "0", 
    et_diff < 2, "0_1m", 
    et_diff < 6, "1_5m", 
    et_diff < 11, "2_10m", 
    et_diff < 16, "3_15m",
    et_diff <= 65, "4_60m", 
    et_diff <= 4*60+10, "5_4h", 
    et_diff <= 24*60+10, "6_24h", 
    et_diff <= 7*24*60+10, "7_7d", 
    et_diff <= 30*24*60+10, "8_30d", 
    et_diff <= 90*24*60+10, "9_90d", 
    1=1, "10_>90d")
}
