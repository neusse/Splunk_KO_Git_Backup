    <panel id="panel_accordion_togglepanel_false_lookups_11" depends="$gadget_lib_loaded$">
      <title>3. Lookup Definitions List</title>
      <table>
        <search base="sh_03_lookups_definitions">
          <query>| table shcluster_label app sharing acl_perms_read acl_perms_write type title filename lookup_file_path size size_KB size_MB collection replicate match_type allow_caching filter index_fields_list min_matches max_matches case_sensitive_match updated author emp_name emp_title emp_dep emp_lob1 emp_type emp_status fields_list 
| `rename_lookup_fields` 
| `rename_identity_fields`</query>
        </search>
