  <search id="sh_03_lookups_definitions">
    <query>| from lookup:splunk_rest_admin_transforms_lookup_sh_kv_store_lookup 
| fields _time shcluster_label Splunk_Roles Splunk_Instance app sharing title filename type collection updated disabled author match_type replicate allow_caching filter index_fields_list min_matches max_matches fields_list case_sensitive_match acl_perms_read acl_perms_write size acl_removable enforceTypes profilingEnabled profilingThresholdMs replication_dump_maximum_size replication_dump_strategy batch_index_query default_match external_cmd external_type reverse_lookup_honor_case_sensitive_match
| lookup splunk_rest_admin_lookup_table_files_sh_kv_store_lookup shcluster_label app filename OUTPUTNEW lookup_file_path
| `get_identity_info(author)` | `fillnull_identity_info`
| `strftime_format(updated)`
| eval Type = if(match(Lookup_File,".csv"), "file", Type) , if(isnull(Lookup_File), "file", Type) , title = if(isnull(title), "NO-DEFINITION", title)
| `gmc_byte2human(size,3)`
| fillnull value="0" min_matches disabled size acl_removable
| fillnull value="100" Max_Matches 
| fillnull value="true" allow_caching replicate case_sensitive_match
| fillnull value="EXACT" match_type
| fillnull value="Undefined" Splunk_Instance Splunk_Roles app author sharing title filename type collection lookup_file_path fields_list
| search shcluster_label IN ($shcluster_label1$) Splunk_Roles IN ($Splunk_Roles1$) Splunk_Roles="*$Splunk_Roles2$*" Splunk_Instance="$Splunk_Instance1$" app="$app1$" title="$title$" author="$author1$" sharing="$sharing1$" filename="$filename$" filename="*$filename_contains$*" lookup_file_path="*$lookup_file_path$*" collection="$collection$" match_type="$match_type$" match_type="*$match_type_contains$*" type="$type3$" case_sensitive_match="$case_sensitive_match$" emp_name="$emp_name$" emp_title="$emp_title$" emp_type="$emp_type$" emp_status="$emp_status$" emp_dep="$emp_dep$" emp_lob1="$emp_lob1$" emp_cc="$emp_cc$" emp_city="$emp_city$" emp_region1="$emp_region1$" emp_country="$emp_country$" fields_list="$fields_list$" size &gt;= $size$ replicate="$replicate$" acl_removable=$acl_removable$ title="*$title_contains$*" lookup_file_path="*$lookup_file_path_contains$*" collection="*$collection_contains$*"
</query>
    <earliest>-15m</earliest>
    <latest>now</latest>
  </search>
  <search id="sh_03_lookups_files">
    <query>
      | from lookup:splunk_rest_admin_lookup_table_files_sh_kv_store_lookup 
| fields _time shcluster_label Splunk_Roles Splunk_Instance app sharing author disabled updated filename lookup_file_path acl_perms_read acl_perms_write acl_removable
| `get_identity_info(author)` | `fillnull_identity_info`
| `strftime_format(updated)`
| search shcluster_label IN ($shcluster_label2$) Splunk_Roles IN ($Splunk_Roles3$) Splunk_Roles="*$Splunk_Roles4$*" Splunk_Instance="$Splunk_Instance2$" app="$app2$" author="$author2$" sharing="$sharing2$" filename="*$filename$*" lookup_file_path="*$lookup_file_path$*" emp_name="$emp_name$" emp_title="$emp_title$" emp_lob1="$emp_lob1$" emp_dep="$emp_dep$" emp_type="$emp_type$" emp_status="$emp_status$" acl_removable=$acl_removable$
    </query>
    <earliest>-15m</earliest>
    <latest>now</latest>
  </search>
  <search id="sh_03_lookups_auto">
    <query>| from lookup:splunk_rest_data_props_lookups_sh_kv_store_lookup 
| fields _time shcluster_label Splunk_Roles Splunk_Instance app sharing stanza attribute transform overwrite value author updated acl_perms_read acl_perms_write acl_removable
| `get_identity_info(shcluster_label,author)` | `get_identity_info(author)` | `fillnull_identity_info`
| `strftime_format(updated)`
| rex field=attribute mode=sed "s/LOOKUP-//g"
| lookup splunk_rest_admin_transforms_lookup_sh_kv_store_lookup shcluster_label app title As transform OUTPUT filename type fields_list collection
| fillnull value="Undefined" Splunk_Instance Splunk_Roles app sharing stanza attribute transform overwrite value author filename
| search shcluster_label IN ($shcluster_label$) Splunk_Instance="$Splunk_Instance$" Splunk_Roles IN ($Splunk_Roles5$) Splunk_Roles="*$Splunk_Roles6$*" app="$app4$" author="$author4$" attribute="$attribute$" stanza="$stanza$" emp_name="$emp_name$" emp_title="$emp_title$" emp_lob1="$emp_lob1$" emp_dep="$emp_dep$" emp_type="$emp_type$" emp_status="$emp_status$" sharing="$sharing$" overwrite="$overwrite$" transform="$transform$" acl_removable=$acl_removable$ filename="*$filename$*"</query>
    <earliest>-15m</earliest>
    <latest>now</latest>
  </search>
