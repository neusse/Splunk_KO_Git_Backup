      <title>4. Lookup Definitions Blacklisting from distsearch.conf</title>
      <table>
        <search>
          <query>| from lookup:splunk_rest_configs_conf_files_kv_store_lookup 
| search config_file="distsearch.conf" stanza="replicationBlacklist" 
| `get_shcluster_label(Splunk_Instance)` 
| makemv delim="###" title 
| mvexpand title 
| search title!="\[*" 
| rex field=title "(?&lt;Property&gt;^\w+)\s*=\s*(?&lt;Value&gt;.*?)$" 
| rex field=Value mode=sed "s/\"//g" 
| stats 
    Values(*) As *
    Dc(Value) As DC_Value
    by shcluster_label app config_file stanza Property 
| table shcluster_label config_file author app sharing stanza Property Value DC_Value 
| where Property!="updated" 
| table shcluster_label app stanza Property Value 
| search shcluster_label IN ($shcluster_label1$) AND app="$app1$"</query>
          <earliest>-15m</earliest>
          <latest>now</latest>
        </search>
        <option name="count">5</option>
        <option name="drilldown">none</option>
        <option name="refresh.display">progressbar</option>
