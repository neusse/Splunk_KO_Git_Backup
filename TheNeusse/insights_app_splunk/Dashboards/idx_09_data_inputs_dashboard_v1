  <description>Splunk HTTP Event Collector (HEC) Inputs Information and All Splunk Instances Data Inputs Information Dashboard</description>
  <search id="idx_09_http_event_collector_1">
    <query>| from lookup:splunk_rest_data_inputs_http_sh_kv_store_lookup
| fields _time,cluster_label,Splu$nk_Roles,Splunk_Instance,app,title,token_name,token,disabled,index_name,indexes,source_name,sourcetype_name,useACK,dedicatedIoThreads,enableSSL,port,maxSockets,maxThreads,rcvbuf,useDeploymentServer
| fillnull value=0 useACK disabled | eval cluster_label = if(cluster_label="No-cluster_label", splunk_server_group, cluster_label) 
| fillnull value="Undefined" app title token_name token index indexes source_name sourcetype_name Splunk_Roles
| search cluster_label IN ($cluster_label$) Splunk_Instance="*$Splunk_Instance$*" Splunk_Roles IN ($Splunk_Roles$) Splunk_Roles="*$Splunk_Roles2$*" app IN ($app$) token_name IN ($token_name$) source_name IN ($source_name$) sourcetype_name IN ($sourcetype_name$) useACK="$useACK$" index_name IN ($index_name$)
</query>
    <earliest>-15m</earliest>
    <latest>now</latest>
  </search>
  <search id="idx_09_http_event_collector_2">
    <query>| tstats ```fillnull_value=undefined```
    Values(data.format) As format
    Max(data.num_of_ack_requests) As num_of_ack_requests
    Max(data.num_of_auth_failures) As num_of_auth_failures
    Max(data.num_of_errors) As num_of_errors
    Max(data.num_of_events) As num_of_events
    Max(data.num_of_parser_errors) As num_of_parser_errors
    Max(data.num_of_requests) As num_of_requests
    Max(data.num_of_requests_acked) As num_of_requests_acked
    Max(data.num_of_requests_in_mint_format) As num_of_requests_in_mint_format
    Max(data.num_of_requests_to_disabled_token) As num_of_requests_to_disabled_token
    Max(data.num_of_requests_to_incorrect_url) As num_of_requests_to_incorrect_url
    Max(data.num_of_requests_waiting_ack) As num_of_requests_waiting_ack
    Values(data.series) As series
    Max(data.total_bytes_indexed) As total_bytes_indexed
    Max(data.total_bytes_received) As total_bytes_received
    Values(data.transport) As transport
    Where index=_introspection sourcetype = http_event_collector_metrics component = HttpEventCollector
    host=$Splunk_Instance$
    By _time host data.token_name
| `gmc_drop_dm_object_name(data)` 
| lookup splunk_rest_data_inputs_http_sh_kv_store_lookup token_name As token_name OUTPUT sourcetype_name app token index_name indexes source_name useACK 
| fillnull value=0 useACK disabled
| `get_idxcluster_label(host)` | `get_instance_roles(host)` | eval search_group = Splunk_Roles | eval cluster_label = if(cluster_label="No-cluster_label", splunk_server_group, cluster_label) 
| fillnull value="Undefined" app title token_name token index indexes source_name sourcetype_name index_name search_group splunk_server_group
| search search_group IN ($cluster_label$) search_group IN ($Splunk_Roles$) token_name IN ($token_name$) index_name IN ($index_name$)
</query>
    <earliest>$TimeRangePkr.earliest$</earliest>
    <latest>$TimeRangePkr.latest$</latest>
  </search>
  <search id="idx_09_http_event_collector_3">
    <query>index=_internal component=HttpInputDataHandler host=$Splunk_Instance$
`gmc_comment("reply=* NOT (channel!=n/a reply=5 http_input_body_size=0) ")`
| fields _time search_group splunk_server_group host name channel reply event_message events_processed http_input_body_size source_IP log_level
| `get_cluster_label(host)` | eval cluster_label = if(cluster_label="No-cluster_label", splunk_server_group, cluster_label) 
| eval search_group = if(isnull(search_group), cluster_label, search_group)
| stats latest(*) as * values(host) as host values(splunk_server_group) As Splunk_Roles by _time search_group name 
| lookup local=True splunk_hec_reply_codes_csv_lookup reply_code AS reply OUTPUT status_message 
| lookup splunk_rest_data_inputs_http_sh_kv_store_lookup token_name As name OUTPUT sourcetype_name app token index_name indexes source_name useACK
| fillnull value=0 useACK disabled
| fillnull value="Undefined" Splunk_Roles app title token_name token index indexes source_name sourcetype_name status_message search_group splunk_server_group
| search search_group IN ($cluster_label$) status_message="$status_message$" search_group IN ($Splunk_Roles$) app IN ($app$) token_name IN ($token_name$) source_name IN ($source_name$) sourcetype_name IN ($sourcetype_name$) useACK="$useACK$"</query>
    <earliest>$TimeRangePkr.earliest$</earliest>
    <latest>$TimeRangePkr.latest$</latest>
  </search>
  <search id="idx_05_inputs">
    <query>| from lookup:splunk_rest_data_inputs_all_kv_store_lookup
| fields _time cluster_label Splunk_Roles Splunk_Instance app title disabled sharing author type description source_name sourcetype_name index_name location acceleration whitelist blacklist crcSalt interval updated connection_host duration endtime exclude group host_resolved id_field host path port queue table
|	`get_identity_info_simple(cluster_label,author)`
| fillnull value="Undefined" Splunk_Instance Splunk_Roles app title disabled sharing author type description source_name sourcetype_name index_name location acceleration whitelist blacklist crcSalt interval
| search cluster_label IN ($cluster_label$) Splunk_Instance="*$Splunk_Instance$*" Splunk_Roles IN ($Splunk_Roles$) Splunk_Roles="*$Splunk_Roles2$*" app="$app$" title="*$title$*" author="$author$" description="*$description$*" source_name="$source_name$" sourcetype_name="$sourcetype_name$" index_name="$index_name$" disabled="$disabled$" type="$type$" emp_name="$emp_name$" emp_title="$emp_title$" emp_lob1="$emp_lob1$" emp_dep="$emp_dep$" emp_type="$emp_type$" emp_status="$emp_status$" sharing="$sharing$"</query>
    <earliest>-15m</earliest>
    <latest>now</latest>
  </search>
  <search id="idx_05_inputs2">
    <query>`setup_summary_index_list` sourcetype=stash source="*inputs.conf"
| rex field=source "(?&lt;folder_name&gt;deployment-apps|master-apps|shcluster|apps|system)" 
| rex field=source "/(deployment-apps|master-apps|shcluster/apps|apps)/(?&lt;app_name&gt;.*?)/" 
| rex field=source "(?&lt;config_file&gt;\w+\.conf)" 
| eval app_name=if(folder_name=="system","system",app_name) , context=case(match(source,"local"), "local", match(source,"default"), "default") 
| fields _time host folder_name app_name context config_file _raw 
| `get_instance_roles(host)`
| stats 
    latest(_time) as _time
    latest(_raw) as file_content
    Values(Splunk_Roles) As Splunk_Roles
    by host folder_name app_name context config_file 
| search host IN ($host$) folder_name IN ($folder_name$) app_name IN ($app_name$) context IN ($context$) Splunk_Roles IN ($Splunk_Roles31$) Splunk_Roles="*$Splunk_Roles32$*"
</query>
    <earliest>$tr1.earliest$</earliest>
    <latest>$tr1.latest$</latest>
  </search>
