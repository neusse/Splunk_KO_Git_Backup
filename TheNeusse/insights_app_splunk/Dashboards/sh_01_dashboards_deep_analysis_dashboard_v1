  <search id="dashboards_base">
    <query>| from lookup:splunk_rest_data_ui_views_sh_kv_store_lookup 
| fields _time,shcluster_label,Splunk_Roles,Splunk_Instance,app,sharing,title,label,description,type,dashboard_size,author,location,updated,time_range,Report_Reference,Dashboard_Reference,Lookup_Reference,Datamodel_Reference,Macro_Reference,Index_Reference,Sourcetype_Reference,Source_Reference,Eventtype_Reference,Rest_Reference,search,BaseSearch,acl_perms_read,acl_perms_write,panel_title,num_panels,auto_refresh_delay,submitButton,autoRun,version
| search NOT app IN ($app21$) NOT title IN ($title21$)
| eval url="http://" . shcluster_label . ":8000/en-US/app/" . app . "/" . title
`gmc_comment("| `get_instance_roles(Splunk_Instance)`")`
| `get_identity_info(author)` | `fillnull_identity_info`
| `get_macro_definition(shcluster_label,Macro_Reference)` | rename Macro_Definition as Macro_Definition1
| rex field=Index_Reference mode=sed "s/`//g"  
| `get_macro_definition(shcluster_label,Index_Reference)` | rename Macro_Definition as Macro_Definition2
| eval Index_Reference = if(isnotnull(Macro_Definition2), Macro_Definition2, Index_Reference) 
| rex field=Index_Reference mode=sed "s/index\s*=\s*//g"
| `get_app_info(app)`
| `strftime_format(updated)`
| eval Num_Searches=mvcount(search)
| eval emp_lob = mvdedup(mvappend(emp_lob1,emp_lob2,emp_lob3,emp_lob4,emp_lob5,emp_lob6))
| eval Effective_autoRun = if(autoRun="false" AND submitButton="true" AND is_time_selector="true", "false", "true" )
| fillnull value=no-emp-info emp_lob emp_name emp_title emp_type emp_status emp_dep emp_cc emp_city emp_region1 emp_country 
| fillnull value=1 version 
| fillnull value=0 num_panels Num_Searches dashboard_size 
| fillnull value="false" autoRun 
| fillnull value="none" submitButton acl_perms_read acl_perms_write auto_refresh_delay panel_title time_range 
| fillnull value="no-reference" Dashboard_Reference Report_Reference Lookup_Reference Datamodel_Reference Macro_Reference Index_Reference Sourcetype_Reference Source_Reference Rest_Reference Eventtype_Reference 
| fillnull value="undefined" shcluster_label Splunk_Roles Splunk_Instance app sharing author label BaseSearch search app_type 
| search shcluster_label IN ($shcluster_label1$) Splunk_Roles IN ($Splunk_Roles1$) Splunk_Roles="*$Splunk_Roles2$*" Splunk_Instance="$Splunk_Instance1$" app IN ($app1$) author=$author1$ sharing IN ($sharing1$) title="$title$" label="$label$" Report_Reference IN ($Report_Reference1$) Dashboard_Reference IN ($Dashboard_Reference1$) Lookup_Reference IN ($Lookup_Reference1$) Datamodel_Reference IN ($Datamodel_Reference1$) Macro_Reference IN ($Macro_Reference1$) Index_Reference IN ($Index_Reference1$) Sourcetype_Reference IN ($Sourcetype_Reference1$) Source_Reference IN ($Source_Reference1$) Eventtype_Reference IN ($Eventtype_Reference1$) Rest_Reference IN ($Rest_Reference1$) BaseSearch="$BaseSearch$" dashboard_size &lt;= $dashboard_size$ search="*$search$*" emp_name="$emp_name$" emp_title="$emp_title$" emp_type="$emp_type$" emp_status="$emp_status$" emp_dep="$emp_dep$" emp_lob IN ($emp_lob$) emp_cc="$emp_cc$" emp_city="$emp_city$" emp_region1="$emp_region1$" emp_country="$emp_country$" time_range="$time_range$" auto_refresh_delay="$auto_refresh_delay$" submitButton="$submitButton$" acl_perms_read="$acl_perms_read$" acl_perms_write="$acl_perms_write$" panel_title="*$panel_title$*" autoRun="$autoRun$" num_panels &gt;= $num_panels$ Num_Searches &gt;= $Num_Searches$ version=$version$ app_type IN ($app_type$) Lookup_Reference = "*$Lookup_Reference2$*"
</query>
    <earliest>-15m</earliest>
    <latest>now</latest>
  </search>
  <search id="tracking_base">
    <query>`setup_summary_index_list` source=splunk_internal_web_access_idx_summary_tracker 
| fields _time, shcluster_label, Splunk_Roles, Splunk_Instance, app, view, user, spent, bytes, sid, useragent, uri_query, emp_*
| eval emp_lob = mvdedup(mvappend(emp_lob1,emp_lob2,emp_lob3,emp_lob4,emp_lob5,emp_lob6)) 
| fillnull value=no-emp-info emp_lob emp_name emp_title emp_type emp_status emp_dep emp_cc emp_city emp_region1 emp_country 
| fillnull value="undefined" shcluster_label Splunk_Roles Splunk_Instance
`gmc_comment("| `get_instance_roles(Splunk_Instance)`")`
| `get_dashboards_info(shcluster_label,app,view)`
| fillnull value="undefined" useragent, uri_query
| search shcluster_label IN ($shcluster_label1$) Splunk_Instance="$Splunk_Instance1$" Splunk_Roles IN ($Splunk_Roles3$) Splunk_Roles="*$Splunk_Roles4$*" app IN ($app3$) view="$title$" uri_query="*$uri_query$*" emp_lob IN ($emp_lob$)</query>
