    <panel id="panel_accordion_togglepanel_false_dashboards_13" depends="$show_single_value$">
      <title>5.9 Macro Definition for: $selected_title$</title>
      <table>
        <search base="dashboards_base">
          <query>| where title="$selected_title$" 
| stats count by shcluster_label, Macro_Reference
| eval Macro_Reference2 = Macro_Reference
| rex field=Macro_Reference2 mode=sed "s/\(.*\)|\$//g"
| eval Macro_Reference = case(match(Macro_Reference, "\(.*,.*,.*,.*\)"), Macro_Reference2 . "(4)", match(Macro_Reference, "\(.*,.*,.*\)"), Macro_Reference2 . "(3)", match(Macro_Reference, "\(.*,.*\)"), Macro_Reference2 . "(2)", match(Macro_Reference, "\(.*\)"), Macro_Reference2 . "(1)", true(), Macro_Reference)
| `get_macro_definition(shcluster_label,Macro_Reference)`
| table Macro_Reference Macro_Definition</query>
        </search>
        <option name="count">1</option>
        <option name="drilldown">none</option>
        <option name="refresh.display">progressbar</option>
        <option name="rowNumbers">true</option>
