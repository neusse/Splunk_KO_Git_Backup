        <fieldForValue>Splunk_Instance</fieldForValue>
        <search base="tracking_base">
          <query>| stats count by Splunk_Instance</query>
        </search>
        <choice value="*">All</choice>
        <default>*</default>
        <initialValue>*</initialValue>
      </input>
      <input type="multiselect" token="app3" searchWhenChanged="false" id="ra_app33">
        <label>5. App</label>
        <fieldForLabel>app3</fieldForLabel>
        <fieldForValue>app</fieldForValue>
        <search base="tracking_base">
          <query>| stats count by app</query>
        </search>
        <choice value="*">All</choice>
        <default>*</default>
        <initialValue>*</initialValue>
        <delimiter>,</delimiter>
      </input>
      <input type="dropdown" token="title" searchWhenChanged="false">
        <label>6. Dashboard Name</label>
        <choice value="*">All</choice>
        <initialValue>*</initialValue>
        <fieldForLabel>title</fieldForLabel>
        <fieldForValue>view</fieldForValue>
        <search base="tracking_base">
          <query>| stats count by view</query>
        </search>
        <default>*</default>
      </input>
      <input type="multiselect" token="emp_lob" searchWhenChanged="false" id="ra_emp_lob4">
        <label>7. Employee LOB</label>
        <fieldForLabel>emp_lob</fieldForLabel>
        <fieldForValue>emp_lob</fieldForValue>
        <search base="tracking_base">
          <query>| stats count by emp_lob</query>
        </search>
        <choice value="*">All</choice>
        <default>*</default>
        <initialValue>*</initialValue>
        <valuePrefix>"</valuePrefix>
        <valueSuffix>"</valueSuffix>
        <delimiter>,</delimiter>
      </input>
      <input type="text" token="uri_query" searchWhenChanged="true">
        <label>8. URI Query</label>
        <initialValue>*</initialValue>
        <default>*</default>
      </input>
      <table>
        <search base="tracking_base">
          <query>| stats 
    Values(user) As Users
    latest(*) as *
    latest(_time) As _time 
    By shcluster_label app view 
| `ustime_format(Latest_Access)` 
| `get_useragent(useragent)` 
| eval label=if(isnull(label), view, label) 
| table _time, shcluster_label, app, label, view, user, spent, bytes, browser_name, browser_version, uri_query
| rename shcluster_label As "SH Cluster Label" app As App, view As "Dashboard ID", label As "Dashboard Name", user As User, spent As "Load Time", bytes As Bytes, browser_name As "Browser Name", browser_version As "Browser Version"</query>
        </search>
        <option name="count">10</option>
        <option name="drilldown">none</option>
        <option name="refresh.display">progressbar</option>
        <option name="rowNumbers">true</option>
        <option name="wrap">false</option>
        <format type="color" field="shcluster_label">
          <colorPalette type="list">[#9BCAEB,#7DBAE5,#5AA7DE,#2D8FD5,#006FBE,#004170]</colorPalette>
          <scale type="sharedCategory"></scale>
        </format>
        <format type="color" field="label">
          <colorPalette type="list">[#9BCAEB,#7DBAE5,#5AA7DE,#2D8FD5,#006FBE,#004170]</colorPalette>
          <scale type="sharedCategory"></scale>
        </format>
        <format type="number" field="Load Time">
          <option name="precision">0</option>
          <option name="unit">ms</option>
        </format>
        <format type="color" field="Load Time">
          <colorPalette type="minMidMax" maxColor="#DC4E41" midColor="#F8BE34" minColor="#53A051"></colorPalette>
          <scale type="minMidMax" midType="percent" midValue="50"></scale>
        </format>
        <format type="color" field="Dashboard Name">
          <colorPalette type="list">[#9BCAEB,#7DBAE5,#5AA7DE,#2D8FD5,#006FBE,#004170]</colorPalette>
          <scale type="sharedCategory"></scale>
        </format>
        <format type="number" field="Bytes">
          <option name="precision">0</option>
          <option name="unit">Bytes</option>
        </format>
        <format type="color" field="SH Cluster Label">
          <colorPalette type="list">[#9BCAEB,#7DBAE5,#5AA7DE,#2D8FD5,#006FBE,#004170]</colorPalette>
          <scale type="sharedCategory"></scale>
        </format>
      </table>
    </panel>
  </row>
