        <title>Dashboards Latest Refresh</title>
        <search base="dashboards_base">
          <query>| stats Latest(_time) As _time 
| fields _time 
| `strftime_format(_time)` 
| rename _time As "splunk_rest_data_ui_views_sh_kv_store Latest Refresh"</query>
        </search>
