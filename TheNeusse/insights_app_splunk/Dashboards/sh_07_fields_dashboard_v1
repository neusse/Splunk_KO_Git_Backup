  <label>SH - 07 - Source Types / Calculated Fields / Field Extractions &amp; Transformations / Field Aliases - Dashboard - v1.1.6</label>
  <description>Splunk Source Types, Calculated Fields, Field Extractions/Transformations and Field Aliases Dashboard</description>
  <search id="sh_07_field_calculated">
    <query>| from lookup:splunk_rest_data_props_calcfields_sh_kv_store_lookup 
  | fields _time shcluster_label Splunk_Roles Splunk_Instance app sharing stanza name value author updated acl_perms_read acl_perms_write acl_removable
  |	`get_identity_info_simple(shcluster_label,author)`
  | `strftime_format(updated)`
  | fillnull value="Undefined" Splunk_Roles Splunk_Instance app sharing stanza name value author
  | search shcluster_label IN ($shcluster_label$) Splunk_Instance IN ($Splunk_Instance$) Splunk_Instance="*$Splunk_Instance_contains$*" Splunk_Roles IN ($Splunk_Roles1$) Splunk_Roles="*$Splunk_Roles2$*" app IN ($app$) author="$author$" name="$name$" stanza="$stanza$" stanza="*$stanza2$*" emp_name="$emp_name$" emp_title="$emp_title$" emp_lob1="$emp_lob1$" emp_dep="$emp_dep$" emp_type IN ($emp_type$) emp_status="$emp_status$" sharing="$sharing$" acl_removable=$acl_removable$</query>
    <earliest>-15m</earliest>
    <latest>now</latest>
  </search>
  <search id="sh_07_field_extractions">
    <query>| from lookup:splunk_rest_data_props_extractions_sh_kv_store_lookup 
| fields _time shcluster_label Splunk_Roles Splunk_Instance app sharing stanza attribute value author type updated acl_perms_read acl_perms_write acl_removable
|	`get_identity_info_simple(shcluster_label,author)`
| `strftime_format(updated)`
| makemv delim="," value
| mvexpand value
| eval value=trim(value)
| lookup splunk_rest_data_transforms_extractions_sh_kv_store_lookup shcluster_label app title as value OUTPUTNEW FORMAT REGEX DELIMS CLEAN_KEYS DEST_KEY FIELDS MV_ADD REPEAT_MATCH SOURCE_KEY WRITE_META
| eval Transform_Type = case(match(attribute, "^REPORT"), "REPORT", match(attribute, "^EXTRACT"), "EXTRACT", true(), attribute)
| rex field=attribute mode=sed "s/^REPORT-|^EXTRACT-//g"
| fillnull value="Undefined" Splunk_Roles Splunk_Instance app sharing stanza attribute value author type Type FORMAT REGEX DELIMS CLEAN_KEYS DEST_KEY FIELDS MV_ADD REPEAT_MATCH SOURCE_KEY WRITE_META
| search shcluster_label IN ($shcluster_label$) Splunk_Instance IN ($Splunk_Instance$) Splunk_Instance="*$Splunk_Instance_contains$*" Splunk_Roles IN ($Splunk_Roles3$) Splunk_Roles="*$Splunk_Roles4$*" app IN ($app$) author="$author$" attribute="$attribute$" type="$type$" stanza="$stanza$" stanza="*$stanza2$*" emp_name="$emp_name$" emp_title="$emp_title$" emp_lob1="$emp_lob1$" emp_dep="$emp_dep$" emp_type IN ($emp_type$) emp_status="$emp_status$" sharing="$sharing$" Transform_Type="$Transform_Type$" acl_removable=$acl_removable$</query>
    <earliest>-15m</earliest>
    <latest>now</latest>
  </search>
  <search id="sh_07_field_aliases">
    <query>| from lookup:splunk_rest_data_props_fieldaliases_sh_kv_store_lookup 
| fields _time shcluster_label Splunk_Roles Splunk_Instance app author stanza attribute value sharing updated acl_perms_read acl_perms_write acl_removable
|	`get_identity_info_simple(shcluster_label,author)` 
| `strftime_format(updated)`
| rex field=attribute mode=sed "s/FIELDALIAS-//g"
| fillnull value="Undefined" Splunk_Roles Splunk_Instance app author stanza attribute value sharing
| search shcluster_label IN ($shcluster_label$) Splunk_Instance IN ($Splunk_Instance$) Splunk_Instance="*$Splunk_Instance_contains$*" Splunk_Roles IN ($Splunk_Roles5$) Splunk_Roles="*$Splunk_Roles6$*" app IN ($app$) author="$author$" attribute="$attribute$" stanza="$stanza$" stanza="*$stanza2$*" emp_name="$emp_name$" emp_title="$emp_title$" emp_lob1="$emp_lob1$" emp_dep="$emp_dep$" emp_type IN ($emp_type$) emp_status="$emp_status$" sharing="$sharing$" acl_removable=$acl_removable$</query>
    <earliest>-15m</earliest>
    <latest>now</latest>
  </search>
  <search id="sh_10_source_types">
    <query>| from lookup:splunk_rest_configs_conf_props_sh_kv_store_lookup
| search NOT app IN ($app_excl_wild$) app IN ($app$) NOT app IN ($app_excl$) title="$title$" sharing="$sharing$" author="$author$"
| fields cluster_label Splunk_Roles Splunk_Instance app title sharing author description TIME_PREFIX TIME_FORMAT MAX_TIMESTAMP_LOOKAHEAD DATETIME_CONFIG SHOULD_LINEMERGE LINE_BREAKER TRUNCATE BREAK_ONLY_BEFORE BREAK_ONLY_BEFORE_DATE MUST_BREAK_AFTER KV_MODE INDEXED_EXTRACTIONS EVENT_BREAKER_ENABLE EVENT_BREAKER ANNOTATE_PUNCT LEARN_SOURCETYPE LEARN_MODEL MAX_EVENTS maxDist updated ADD_EXTRA_TIME_FIELDS AUTO_KV_JSON CHARSET CHECK_FOR_HEADER CHECK_METHOD DEPTH_LIMIT FIELD_DELIMITER FIELD_HEADER_REGEX HEADER_FIELD_DELIMITER HEADER_MODE JSON_TRIM_BRACES_IN_ARRAY_NAMES LINE_BREAKER_LOOKBEHIND MATCH_LIMIT MAX_DAYS_AGO MAX_DAYS_HENCE MAX_DIFF_SECS_AGO MAX_DIFF_SECS_HENCE METRIC-SCHEMA-TRANSFORMS METRICS_PROTOCOL MUST_NOT_BREAK_AFTER MUST_NOT_BREAK_BEFORE NO_BINARY_CHECK PREFIX_SOURCETYPE TIMESTAMP_FIELDS TRANSFORMS TZ category disabled invalid_cause is_valid priority pulldown_type rename 

| `get_identity_info(cluster_label,author)` | `get_identity_info(author)` | `fillnull_identity_info`
| `strftime_format(updated)`

| fillnull value="auto" KV_MODE 
| fillnull value="128" MAX_TIMESTAMP_LOOKAHEAD 
| fillnull value="300" maxDist 
| fillnull value="256" MAX_EVENTS 
| fillnull value="0" EVENT_BREAKER_ENABLE CHECK_FOR_HEADER FIELD_HEADER_REGEX JSON_TRIM_BRACES_IN_ARRAY_NAMES NO_BINARY_CHECK
| fillnull value="1" ANNOTATE_PUNCT LEARN_MODEL SHOULD_LINEMERGE LEARN_SOURCETYPE BREAK_ONLY_BEFORE_DATE ADD_EXTRA_TIME_FIELDS AUTO_KV_JSON PREFIX_SOURCETYPE is_valid
| fillnull value="10000" TRUNCATE 
| fillnull value="^" TIME_PREFIX 
| fillnull value="([\r\n]+)" LINE_BREAKER 
| fillnull value="\r\n" EVENT_BREAKER 
| fillnull value="/etc/datetime.xml" DATETIME_CONFIG 
| fillnull value="not-set" INDEXED_EXTRACTIONS 
| fillnull value="UTF-8" CHARSET 
| fillnull value="endpoint_md5" CHECK_METHOD 
| fillnull value="1000" DEPTH_LIMIT 
| fillnull value="100" LINE_BREAKER_LOOKBEHIND 
| fillnull value="100000" MATCH_LIMIT 
| fillnull value="2000" MAX_DAYS_AGO 
| fillnull value="2" MAX_DAYS_HENCE 
| fillnull value="3600" MAX_DIFF_SECS_AGO 
| fillnull value="604800" MAX_DIFF_SECS_HENCE
| fillnull value="undefined" Splunk_Roles

| search cluster_label IN ($cluster_label$) Splunk_Instance IN ($Splunk_Instance$) Splunk_Instance="*$Splunk_Instance_contains$*" Splunk_Roles IN ($Splunk_Roles$) Splunk_Roles="*$Splunk_Roles2$*" MAX_TIMESTAMP_LOOKAHEAD="$MAX_TIMESTAMP_LOOKAHEAD$" SHOULD_LINEMERGE="$SHOULD_LINEMERGE$" EVENT_BREAKER_ENABLE="$EVENT_BREAKER_ENABLE$" TRUNCATE="$TRUNCATE$" LEARN_SOURCETYPE="$LEARN_SOURCETYPE$" KV_MODE="$KV_MODE$" INDEXED_EXTRACTIONS="$INDEXED_EXTRACTIONS$"
emp_status="$emp_status$" emp_type IN ($emp_type$) emp_dep="$emp_dep$" emp_lob1="$emp_lob1$" emp_lob2="$emp_lob2$" emp_lob3="$emp_lob3$" emp_title="$emp_title$" emp_cc="$emp_cc$" emp_city="$emp_city$" emp_region1="$emp_region1$" emp_country="$emp_country$"</query>
    <earliest>-15m</earliest>
    <latest>now</latest>
  </search>
