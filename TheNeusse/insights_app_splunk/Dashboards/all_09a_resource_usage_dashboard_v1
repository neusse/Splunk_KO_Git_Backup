  <label>IA4S - 09A - Resource Usage Analysis - Dashboard - v1.1.6</label>
  <description>Splunk Resource Usage Information Dashboard</description>
  <search id="hostwide_base">
    <query>
      | tstats
    P99(data.cpu_system_pct) As cpu_system_pct
    P99(data.cpu_user_pct) As cpu_user_pct 
    P99(data.cpu_idle_pct) As cpu_idle_pct
    P99(data.forks) As forks
    P99(data.runnable_process_count) As runnable_process_count
    P99(data.mem_used) As mem_used
    Max(data.cpu_count) As cpu_count
    Max(data.mem) As mem
    Max(data.pg_swapped_out) As pg_swapped_out
    Max(data.normalized_load_avg_1min) As normalized_load_avg_1min
    Max(data.swap) As swap
    Max(data.pg_paged_out) As pg_paged_out
    Max(data.swap_used) As swap_used
    Max(data.virtual_cpu_count) As virtual_cpu_count
    Latest(data.cpu_arch) As cpu_arch
    Latest(data.os_build) As os_build
    Latest(data.os_name) As os_name
    Latest(data.os_name_ext) As os_name_ext
    Latest(data.os_version) As os_version
    Where index=_introspection sourcetype=splunk_resource_usage ```component=Hostwide```
    `setup_all_servers_search`
    host IN ($Hostwide_Splunk_Instance$)
    By _time span=10s component host
| where component="Hostwide"
| fields _time host cpu_system_pct cpu_user_pct cpu_idle_pct forks runnable_process_count mem_used cpu_count mem pg_swapped_out normalized_load_avg_1min swap pg_paged_out swap_used virtual_cpu_count cpu_arch os_build os_name os_name_ext os_version
| `gmc_drop_dm_object_name(data)`
| `get_cluster_label(host)` | `get_instance_roles(host)` 
| eval cpu_total_pct = cpu_user_pct + cpu_system_pct , mem_used_gb = round(mem_used / 1024, 2)
| search cluster_label IN ($Hostwide_cluster_label$) Splunk_Roles IN ($Hostwide_Splunk_Roles$) Splunk_Roles = "$Hostwide_Splunk_Roles2$"
</query>
    <earliest>$TimeRangePkr1.earliest$</earliest>
    <latest>$TimeRangePkr1.latest$</latest>
  </search>
  <search id="process_base">
    <query>
$control_token_process$ | tstats fillnull_value="na"
    P99(data.normalized_pct_cpu) As normalized_pct_cpu 
    P99(data.fd_used) As fd_used 
    P99(data.mem_used) As mem_used
    P99(data.t_count) As t_count
    P99(data.elapsed) As total_run_time 
    P99(data.search_props.delta_scan_count) As delta_scan_count
    P99(data.search_props.scan_count) As scan_count
    P99(data.page_faults) As page_faults
    P99(data.pct_cpu) As pct_cpu
    P99(data.pct_memory) As pct_memory
    P99(data.read_mb) As read_mb
    P99(data.written_mb) As written_mb 
    Latest(data.search_props.provenance) As provenance 
    Latest(data.status) As status
    Latest(data.search_props.search_head) As search_head
    Latest(data.workload_pool) As workload_pool
    Latest(data.workload_pool_cpu_shares) As workload_pool_cpu_shares
    Latest(data.workload_pool_mem_limit) As workload_pool_mem_limit
    Latest(data.workload_pool_type) As workload_pool_type
    Latest(data.search_props.label) As savedsearch_name
    Latest(data.pid) As pid
    Latest(data.ppid) As ppid
    Latest(data.search_props.role) As role
    Latest(_time) As _time
    Where index=_introspection sourcetype=splunk_resource_usage ```component=PerProcess```
    `setup_all_servers_search`
    By component host data.process_type data.search_props.sid data.search_props.mode data.search_props.type data.search_props.app data.search_props.user
| where component="PerProcess"
| `gmc_drop_dm_object_name(data.search_props)` 
| `gmc_drop_dm_object_name(data)` 
| rename sid As search_id
| fields _time host process_type search_id search_head app user delta_scan_count total_run_time fd_used mem_used normalized_pct_cpu pct_cpu page_faults pid ppid provenance read_mb savedsearch_name scan_count type t_count written_mb mode workload_pool_type workload_pool workload_pool_cpu_shares workload_pool_mem_limit role
| fillnull value=0 normalized_pct_cpu mem_used fd_used t_count total_run_time pct_cpu page_faults scan_count written_mb read_mb
| fillnull value="na" role user app type process_type
| search host IN ($PerProcess_Splunk_Instance$) process_type IN ($PerProcess_process_type$) app IN ($app$) user IN ($user$) type IN ($type$) role IN ($role$) fd_used&gt;$fd_used$ mem_used&gt;=$mem_used$ t_count&gt;=$t_count$ total_run_time&gt;=$total_run_time$
| `get_cluster_label(host)` 
| `get_instance_roles(host)` 
| `get_app_info(app)` 
| `get_provenance_fields(provenance)` 
| `get_identity_info(user)`
```| join type=left host 
    [| tstats count where index=_introspection `setup_all_servers_search` earliest=-4h by splunk_server 
    | rename splunk_server as host 
    | eval Splunk_Roles=mvappend(Splunk_Roles,"indexer") ] 
| join type=left host 
    [| tstats count Where index=_introspection sourcetype=splunk_resource_usage component=PerProcess data.process_type=search `setup_all_servers_search` earliest=-4h by data.search_props.search_head 
    | rename data.search_props.search_head as host 
    | eval Splunk_Roles=mvappend(Splunk_Roles,"search_head")] ```
| eval Splunk_Roles = mvdedup(Splunk_Roles) 
| eval process_type_normalized=case( match(process_type,"^search"),"search", match(process_type,"^index_service") OR Splunk_Roles=="indexer" ,"ingestion", Splunk_Roles=="search_head","shared_services", true(), process_type)
| eval dm_name = if(app="Splunk_SA_CIM" AND app!="N/A", savedsearch_name, null() )
| rex field=dm_name mode=sed "s/_ACCELERATE_DM_Splunk_SA_CIM_|_ACCELERATE_//g"
| rex field=dm_name "(?&lt;dm_name&gt;^\w+)"
| rex field=provenance mode=sed "s/^UI:Dashboard://g"
| eval savedsearch_name =if(isnotnull(dm_name) AND app="Splunk_SA_CIM", dm_name, savedsearch_name), mem_used_mb = mem_used, mem_used_gb = round(mem_used / 1024 / 1024, 2)
| rex field=savedsearch_name mode=sed "s/^_ACCELERATE_DM_|^_ACCELERATE_\S{8}-\S{4}-\S{4}-\S{4}-\S{12}_|_ACCELERATE_$//g"

| fillnull value="na" savedsearch_name search_head Dashboard_Name cluster_label app_type workload_pool_type workload_pool mode Provenance_Type provenance
| search cluster_label IN ($PerProcess_cluster_label$) Splunk_Roles IN ($PerProcess_Splunk_Roles$) Splunk_Roles = "$PerProcess_Splunk_Roles2$"
Dashboard_Name IN ($Dashboard_Name$) app_type IN ($app_type$) workload_pool_type IN ($workload_pool_type$) workload_pool IN ($workload_pool$) savedsearch_name IN ($savedsearch_name$) search_head IN ($search_head$) Provenance_Type IN ($Provenance_Type$) mode IN ($mode$) provenance="$provenance$" normalized_pct_cpu &gt;=$normalized_pct_cpu$ scan_count&gt;=$scan_count$
    </query>
    <earliest>$TimeRangePkr2.earliest$</earliest>
    <latest>$TimeRangePkr2.latest$</latest>
  </search>
  <search id="iowait_base">
    <query>
      $control_token_iowait$ | tstats
    P99(data.avg_cpu_pct) As avg_cpu_pct
    P99(data.max_cpus_pct) As max_cpus_pct 
    Where index=_introspection sourcetype=splunk_resource_usage ```component=IOWait```
    `setup_all_servers_search`
    host IN ($IOWait_Splunk_Instance$)
    By _time span=10s component, host
| where component="IOWait"
| `gmc_drop_dm_object_name(data)` 
| `get_cluster_label(host)` 
| `get_instance_roles(host)`
| search cluster_label IN ($IOWait_cluster_label$) Splunk_Roles IN ($IOWait_Splunk_Roles$) Splunk_Roles = "$IOWait_Splunk_Roles2$"
      </query>
    <earliest>$TimeRangePkr3.earliest$</earliest>
    <latest>$TimeRangePkr3.latest$</latest>
  </search>
  <search id="iostats_base">
    <query>
$control_token_iostats$ | tstats
    P99(data.avg_service_ms) As avg_service_ms
    P99(data.avg_total_ms) As avg_total_ms 
    P99(data.cpu_pct) As cpu_pct 
    P99(data.interval) As interval 
    P99(data.reads_kb_ps) As reads_kb_ps 
    P99(data.reads_ps) As reads_ps 
    P99(data.writes_kb_ps) As writes_kb_ps 
    P99(data.writes_ps) As writes_ps 
    Where index=_introspection sourcetype=splunk_resource_usage ```component=IOStats```
    `setup_all_servers_search`
    host IN ($IOStats_Splunk_Instance$)
    By _time span=10s component, host
| where component="IOStats"
| `gmc_drop_dm_object_name(data)` 
| `get_cluster_label(host)` 
| `get_instance_roles(host)`* 
| search cluster_label IN ($IOStats_cluster_label$) Splunk_Roles IN ($IOStats_Splunk_Roles$) Splunk_Roles = "$IOStats_Splunk_Roles2$"
      </query>
    <earliest>$TimeRangePkr4.earliest$</earliest>
    <latest>$TimeRangePkr4.latest$</latest>
  </search>
  <search id="diskio_base">
    <query>
      $control_token_diskio$ | tstats 
    P99(data.avg_service_ms) As avg_service_ms
    P99(data.avg_total_ms) As avg_total_ms 
    P99(data.cpu_pct) As cpu_pct 
    P99(data.interval) As interval 
    P99(data.reads_kb_ps) As reads_kb_ps 
    P99(data.reads_ps) As reads_ps 
    P99(data.writes_kb_ps) As writes_kb_ps 
    P99(data.writes_ps) As writes_ps 
    P99(data.avg_cpu_pct) As avg_cpu_pct
    P99(data.max_cpus_pct) As max_cpus_pct 
    Where index=_introspection sourcetype=splunk_resource_usage ```component IN (IOStats,IOWait)```
    earliest=$TimeRangePkr.earliest$
    latest=$TimeRangePkr.latest$
    `setup_all_servers_search`
    host IN (*)
    By _time span=10s component, host
| where component="IOStats" OR component="IOWait"
| `gmc_drop_dm_object_name(data)` 
| `get_cluster_label(host)` 
| `get_instance_roles(host)`
| search Splunk_Roles IN ($Splunk_Roles$) host="*$host$*"
        </query>
    <earliest>$TimeRangePkr.earliest$</earliest>
    <latest>$TimeRangePkr.latest$</latest>
  </search>
