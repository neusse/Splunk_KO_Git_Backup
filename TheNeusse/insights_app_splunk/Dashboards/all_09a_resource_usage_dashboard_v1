  <row>
    <panel id="panel_accordion_togglepanel_true_99" depends="$gadget_lib_loaded$">
      <title>3. Metric Information from REST API Manual</title>
      <input type="multiselect" token="Component_Lookup" searchWhenChanged="true" id="ra_Component_Lookup">
        <label>1. Component</label>
        <fieldForLabel>Component_Lookup</fieldForLabel>
        <fieldForValue>Component</fieldForValue>
        <search id="metric_info">
          <query>| inputlookup splunk_gmc_kb_csv_lookup where Index="Introspection" | stats count by Component</query>
          <earliest>-24h@h</earliest>
          <latest>now</latest>
        </search>
        <choice value="*">All</choice>
        <default>*</default>
        <initialValue>*</initialValue>
        <delimiter>,</delimiter>
      </input>
      <input type="multiselect" token="Field_Name" searchWhenChanged="true" id="ra_Field_Name">
        <label>2. Field_Name</label>
        <fieldForLabel>Field_Name</fieldForLabel>
        <fieldForValue>Field_Name</fieldForValue>
        <search id="ia4s_kb1">
          <query>| inputlookup splunk_gmc_kb_csv_lookup where Index="Introspection" AND Component IN ($Component_Lookup$) | stats count by Field_Name</query>
          <earliest>-24h@h</earliest>
          <latest>now</latest>
        </search>
        <choice value="*">All</choice>
        <default>*</default>
        <initialValue>*</initialValue>
        <delimiter>,</delimiter>
      </input>
      <table>
        <search id="ia4s_kb2">
          <query>| inputlookup splunk_gmc_kb_csv_lookup where Index="Introspection" | search Component IN ($Component_Lookup$) Field_Name IN ($Field_Name$) | table Component Field_Name Field_Display_Name Field_Description Default | rename Default As "Component Description"</query>
          <earliest>-15m</earliest>
          <latest>now</latest>
        </search>
        <option name="count">5</option>
        <option name="drilldown">none</option>
        <option name="refresh.display">progressbar</option>
        <option name="rowNumbers">true</option>
        <option name="wrap">true</option>
        <format type="color" field="Component">
          <colorPalette type="list">[#9BCAEB,#7DBAE5,#5AA7DE,#2D8FD5,#006FBE,#004170]</colorPalette>
          <scale type="sharedCategory"></scale>
        </format>
      </table>
    </panel>
  </row>
