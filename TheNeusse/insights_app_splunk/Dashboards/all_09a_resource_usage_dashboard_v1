    <panel id="panel_accordion_togglepanel_false_Hostwide_3" depends="$gadget_lib_loaded$">
      <title>2.2 Hostwide | $Hostwide_Function$ $Hostwide_Metric$ $Hostwide_Splitby$</title>
      <chart>
        <title>From: $TimeRangePkr1.earliest$ To: $TimeRangePkr1.latest$</title>
        <search base="hostwide_base">
          <progress>
            <eval token="duration1">tostring(tonumber($job.runDuration$),"duration")</eval>
          </progress>
