  <row id="tab1_panel0">
    <panel>
      <table>
        <search>
          <query>| makeresults 
| eval title=mvappend(title,"insights_app_splunk") 
| stats count by title 
| join title type=left 
    [| rest /servicesNS/-/-/apps/local splunk_server=local 
    | search title IN (insights_app_splunk) 
    | fields title, label, disabled, version, update.version] 
| rename version AS current_version update.version AS latest_version 
| eval message=case(disabled=1, "App is disabled!", isnotnull(current_version) AND isnull(latest_version), "Version: \"" . current_version . "\" is the latest version", isnotnull(latest_version), "You are currently running an older version: \"" . current_version . "\" of this App.  A new version: \"" . latest_version . "\" is available", true(), "App is not installed on this search head!") 
| eval label = case(
    title="insights_app_splunk", "Insights App for Splunk", 
    true(), label) 
| eval status=if(isnotnull(label) AND disabled=0, "installed", "missing") 
| eval app_id = case(title="insights_app_splunk",6489) 
| eval url = case(isnotnull(current_version) AND isnull(latest_version) , "https://splunkbase.splunk.com/app/" . app_id, isnotnull(latest_version) , "/manager/appinstall/" . title, true(), "/manager/search/appsremote?query=" . label) 
| eval latest_version = if(isnull(latest_version) OR latest_version="", current_version, latest_version) 
| fillnull value="NOT FOUND" latest_version current_version 
| table label status message current_version latest_version url 
| rename label as "App Name", message As "App version check", current_version as "Installed Version", latest_version as "Latest Version"</query>
          <earliest>-24h@h</earliest>
          <latest>now</latest>
        </search>
        <option name="drilldown">cell</option>
        <option name="refresh.display">progressbar</option>
        <option name="rowNumbers">true</option>
        <option name="wrap">false</option>
        <format type="color" field="App version check">
          <colorPalette type="expression">case(match(value,"You are currently running an older version"), "#B50101", match(value,"You are currently running an older Pre-req version"), "#F26A35", match(value,"^Version"),"#5CC05C", true(), "#B50101")</colorPalette>
        </format>
        <format type="color" field="status">
          <colorPalette type="map">{"installed":#5CC05C,"missing":#B50101}</colorPalette>
        </format>
        <format type="color" field="notes">
          <colorPalette type="sharedList"></colorPalette>
          <scale type="sharedCategory"></scale>
        </format>
        <fields>["App Name","App version check","Installed Version","Latest Version"]</fields>
        <drilldown>
          <link target="_blank">$row.url|n$</link>
        </drilldown>
      </table>
    </panel>
