  <row>
    <panel id="panel_accordion_togglepanel_false_23" depends="$gadget_lib_loaded$">
      <title>10.4 Search Statistics by Indexer Cluster and the search heads executing these searches yesterday</title>
      <table>
        <search>
          <query>| tstats Latest(data.search_props.search_head) As search_head 
    Where index=_introspection sourcetype=splunk_resource_usage component=PerProcess 
    data.process_type=search data.search_props.sid = "remote_*" 
    `setup_indexer_search` 
    earliest=-1d@d latest=-0d@d
    By host data.search_props.sid data.search_props.mode data.search_props.type data.search_props.app data.search_props.user 
| `gmc_drop_dm_object_name(data.search_props)` 
| `gmc_drop_dm_object_name(data)` 
| `get_shcluster_label(search_head)` 
| `get_idxcluster_label(host)` 
| eval shcluster_label = if(match(search_head, "-brdg\d+_"), "oi_prod_amrs_shc_core", shcluster_label) 
| eval shcluster_label = if(shcluster_label="No-shcluster_label" OR isnull(shcluster_label), search_head, shcluster_label) 
| eval elapsed = if(mode="RT", 0, elapsed) 
| stats dc(sid) as count by shcluster_label idxcluster_label type 
| stats sum(count) as num_search_by_idx_sh_type by shcluster_label idxcluster_label type 
| eventstats sum(num_search_by_idx_sh_type) as num_search_by_sh by shcluster_label idxcluster_label 
| eventstats sum(num_search_by_idx_sh_type) as num_search_by_idx by idxcluster_label 
| table shcluster_label idxcluster_label type num_search_by_idx_sh_type num_search_by_sh num_search_by_idx</query>
          <earliest>-24h</earliest>
          <latest>now</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="count">10</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">none</option>
        <option name="percentagesRow">false</option>
        <option name="refresh.display">progressbar</option>
        <option name="rowNumbers">true</option>
        <option name="totalsRow">false</option>
        <option name="wrap">false</option>
        <format type="number" field="num_search_by_idx">
          <option name="precision">0</option>
        </format>
        <format type="color" field="shcluster_label">
          <colorPalette type="list">[#9BCAEB,#7DBAE5,#5AA7DE,#2D8FD5,#006FBE,#004170]</colorPalette>
          <scale type="sharedCategory"></scale>
        </format>
        <format type="color" field="idxcluster_label">
          <colorPalette type="list">[#9BCAEB,#7DBAE5,#5AA7DE,#2D8FD5,#006FBE,#004170]</colorPalette>
          <scale type="sharedCategory"></scale>
        </format>
        <format type="color" field="num_search_by_idx">
          <colorPalette type="minMidMax" maxColor="#DC4E41" midColor="#F8BE34" minColor="#53A051"></colorPalette>
          <scale type="minMidMax" midType="percentile" midValue="50"></scale>
        </format>
        <format type="number" field="num_search_by_sh">
          <option name="precision">0</option>
        </format>
        <format type="number" field="num_search_by_idx_sh_type">
          <option name="precision">0</option>
        </format>
      </table>
    </panel>
  </row>
