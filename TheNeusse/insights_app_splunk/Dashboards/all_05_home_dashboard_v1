    <panel id="panel_accordion_togglepanel_false_3">
      <title>2.2 Users didn't access the Platform in the last $Days_Latest_Access_2b$ days</title>
      <input type="text" token="Days_Latest_Access_2b" searchWhenChanged="true">
        <label>1. Days Since Latest Access</label>
        <default>30</default>
        <initialValue>30</initialValue>
      </input>
      <table>
        <search>
          <query>`setup_summary_index_list` source=splunk_audit_user_login_idx_summary_tracker NOT user IN (int_*) NOT emp_type IN (Internal, Vendor) emp_status!=Terminated earliest=-$Days_Latest_Access_2b$d@d 
| fields _time user emp_name emp_dep emp_lob emp_status emp_type 
| stats latest(_time) as _time latest(*) as * by user 
| eval days_since_last_access = round((now() - _time) / 86400 , 0) 
| rename _time As Latest_Login 
| convert ctime(Latest_Login) 
| table user emp_name Latest_Login days_since_last_access emp_status emp_type emp_dep emp_lob 
| where days_since_last_access &gt; $Days_Latest_Access_2b$ 
| sort 0 - days_since_last_access</query>
          <earliest>-24h</earliest>
          <latest>now</latest>
        </search>
        <option name="drilldown">none</option>
        <option name="refresh.display">progressbar</option>
        <option name="rowNumbers">true</option>
        <option name="wrap">false</option>
        <format type="color" field="user">
          <colorPalette type="list">[#9BCAEB,#7DBAE5,#5AA7DE,#2D8FD5,#006FBE,#004170]</colorPalette>
          <scale type="sharedCategory"></scale>
        </format>
        <format type="color" field="days_since_last_access">
          <colorPalette type="minMidMax" maxColor="#DC4E41" midColor="#F1813F" minColor="#F8BE34"></colorPalette>
          <scale type="minMidMax" midType="percent" midValue="50"></scale>
        </format>
      </table>
      <single>
        <search>
          <query>`setup_summary_index_list` source=splunk_audit_user_login_idx_summary_tracker NOT user IN (int_*) NOT emp_type IN (Internal, Vendor) emp_status!=Terminated earliest=-$Days_Latest_Access_2b$d@d 
| fields _time user emp_name emp_dep emp_lob emp_status emp_type 
| stats latest(_time) as _time latest(*) as * by user 
| eval days_since_last_access = round((now() - _time) / 86400 , 0) 
| rename _time As Latest_Login 
| convert ctime(Latest_Login) 
| table user emp_name Latest_Login days_since_last_access emp_status emp_type emp_dep emp_lob 
| where days_since_last_access &gt; $Days_Latest_Access_2b$ 
| stats count</query>
          <earliest>-24h</earliest>
