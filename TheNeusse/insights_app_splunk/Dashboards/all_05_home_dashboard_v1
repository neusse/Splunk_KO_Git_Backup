      <table>
        <search>
          <query>| tstats SUMMARIESONLY=TRUE ALLOW_OLD_SUMMARIES=FALSE
    count
    Latest(Search_Activity.Audit_Search.emp_dep1) As emp_dep
    Latest(Search_Activity.Audit_Search.emp_name1) As emp_name
    Latest(Search_Activity.Audit_Search.emp_lob1) As emp_lob
    Latest(Search_Activity.Audit_Search.emp_type1) As emp_type
    Latest(Search_Activity.Audit_Search.emp_status1) As emp_status
    FROM DataModel=IA4S
    WHERE nodename=Search_Activity.Audit_Search 
    AND index=_audit 
    AND Search_Activity.Audit_Search.search_type1 = "adhoc"
    AND Search_Activity.Audit_Search.provenance1 = "UI:*"
    NOT Search_Activity.Audit_Search.user1 IN (admin, splunk-system-user)
    NOT Search_Activity.Audit_Search.provenance1 IN (*$exclusions_2$*)
    earliest=-$Days_Latest_Access_3$d
    BY host Search_Activity.Audit_Search.user1 
| `gmc_drop_dm_object_name(Search_Activity.Audit_Search)` 
| rename user1 As user 
| `get_shcluster_info(host)` 
| search NOT emp_type IN (service) 
| `fillnull_identity_info` 
| table shcluster_label user emp_name emp_title emp_dep emp_lob emp_type emp_status 
| `rename_identity_fields` 
| `rename_common_fields`</query>
          <earliest>-24h</earliest>
          <latest>now</latest>
        </search>
