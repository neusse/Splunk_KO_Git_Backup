          <eval token="form.SearchRunEarliest">if(strftime($SelectedTime.earliest$,"%Y-%m-%dT%H:%M:%S.%3N")!="Invalid date",strftime($SelectedTime.earliest$,"%Y-%m-%dT%H:%M:%S.%3N"),if(strptime($SelectedTime.earliest$,"%Y-%m-%dT%H:%M:%S.%3N")!="Invalid date",$SelectedTime.earliest$,strftime(relative_time(now(),$SelectedTime.earliest$),"%Y-%m-%dT%H:%M:%S.%3N")))</eval>
