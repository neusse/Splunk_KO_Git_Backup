      <chart rejects="$LoadingPerfStats$">
        <title></title>
        <search base="MainSearchPerformance">
          <query>
|  fields *command.*.input_count *command.*.output_count *command.*.invocations
| foreach *.input_count
   [eval INPUT_LIST = mvappend(if(isnull('&lt;&lt;FIELD&gt;&gt;'), "FIELD:" + "&lt;&lt;FIELD&gt;&gt;" + "| VAL:" + "0","FIELD:" + "&lt;&lt;FIELD&gt;&gt;" + "| VAL:" + '&lt;&lt;FIELD&gt;&gt;'),INPUT_LIST)
   ]
| foreach *.output_count
   [eval OUTPUT_LIST = mvappend(if(isnull('&lt;&lt;FIELD&gt;&gt;'),"FIELD:" + "&lt;&lt;FIELD&gt;&gt;" + "| VAL:" + "0","FIELD:" + "&lt;&lt;FIELD&gt;&gt;" + "| VAL:" + '&lt;&lt;FIELD&gt;&gt;'),OUTPUT_LIST)
   ]
| foreach *.invocations
   [eval INVOCATION_LIST = mvappend(if(isnull('&lt;&lt;FIELD&gt;&gt;'),"FIELD:" + "&lt;&lt;FIELD&gt;&gt;" + "| VAL:" + "0","FIELD:" + "&lt;&lt;FIELD&gt;&gt;" + "| VAL:" + '&lt;&lt;FIELD&gt;&gt;'),INVOCATION_LIST)
   ]
| eval FIELD_LIST = mvzip(mvzip(INPUT_LIST,OUTPUT_LIST,":|:"),INVOCATION_LIST,":|:")
| table FIELD_LIST
| mvexpand FIELD_LIST
| rex field=FIELD_LIST "^FIELD:(\w+\.){2}(?&lt;Command&gt;\w+)\.input_count\|\sVAL:(?&lt;Input&gt;\d+):\|:FIELD:(\w+\.){3}output_count\|\sVAL:(?&lt;Output&gt;\d+):\|:FIELD:(\w+\.){3}invocations\|\sVAL:(?&lt;Invocations&gt;\d+)"
| search Input&gt;0 OR Output&gt;0
| fields - FIELD_LIST</query>
        </search>
        <option name="charting.seriesColors">[0x$color_chart_primary_1$,0x$color_chart_secondary_1$]</option>
