          </div>
        </div>
      </html>
      <table depends="$ShowAuditAnalysis$">
        <search>
          <query>
            index=_audit sourcetype=audittrail search_id=$MainSID$ user!=splunk-system-user host=*splunk* `debugComment("ISINDEBUG")`
            | transaction search_id
            | search search!="'| metadata type=* | search totalCount&gt;0"
            | regex search!="^'?(typeahead|\||history|inputlookup|search \(?`(\w|_)+.*`\)?)"
            | eval search_window=round((api_lt-api_et)/60/60/24)
            | eval behavior=mvappend(if(search_window &gt;= 30, "Long Search Window", null()),if(not match(search,"index.?(=|::)"), "Missing Index", null()), if(not match(search,"sourcetype.?(=|::)"), "Missing Sourcetype",null()), if(not match(search,"host.?(=|::)"), "Missing Host", null())), sqs=if(search_window &gt;= 30, 7, 0) + if(not match(search,"index.?(=|::)"), 13, 0) + if(not match(search,"sourcetype.?(=|::)"), 3,0) + if(not match(search,"host.?(=|::)"), 2,0)
            | where sqs&gt;0
            | sort - sqs total_run_time
            | table user, search, scan_count, search_window, total_run_time, sqs, behavior
            | eval "Search Duration"=tostring(total_run_time, "duration")
            | rename user as "User", search as "Search SPL", scan_count as "Events Scanned", sqs as "Splunk Query Score *", behavior as "Potentially Inefficient Behavior", search_window as "Search Time Range (days)"
            | fieldformat "Events Scanned"=toString('Events Scanned', "commas")
            | fieldformat "Search Time Range (days)"=toString('Search Time Range (days)', "commas")
            | fields "User", "Search SPL", "Events Scanned", "Search Time Range (days)", "Search Duration", "Splunk Query Score *", "Potentially Inefficient Behavior"
            
          </query>
          <progress>
            <unset token="ShowAuditAnalysis"></unset>
          </progress>
          <done>
            <condition match="$job.resultCount$ &gt; 0">
              <set token="ShowAuditAnalysis"></set>
            </condition>
          </done>
        </search>
      </table>
