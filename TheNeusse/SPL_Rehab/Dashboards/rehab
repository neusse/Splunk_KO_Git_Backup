  <search id="MainSearchForPerf">
    <query>$form.EnteredSearch$ $RunManualSearch$ |`debugComment("ISINDEBUG")` addinfo | eval info_sid = replace(info_sid,"^remote\_[^_]+\_","")</query>
    <earliest>$SelectedTime.earliest$</earliest>
    <latest>$SelectedTime.latest$</latest>
    <done>
      <set token="MainSID">$result.info_sid$</set>
      <unset token="LoadingMainPerfStats"></unset>
      <set token="MainSIDObtained"></set>
      <set token="ManuallyRun"></set>
    </done>
  </search>
  <search id="MainSearchForAnalysis">
    <query>
      $RunSearch$ `debugComment("ISINDEBUG")` addinfo
    </query>
    <earliest>$SelectedTime.earliest$</earliest>
    <latest>$SelectedTime.latest$</latest>
    <progress>
      <unset token="SID"></unset>
      <set token="LoadingPerfStats"></set>
      <unset token="SubSIDObtained"></unset>
    </progress>
    <done>
      <set token="SID">$result.info_sid$</set>
      <unset token="LoadingPerfStats"></unset>
      <set token="SubSIDObtained"></set>
    </done>
  </search>
  <search id="MainTotalStats">
    <query>
      |loadjob $MainSID$| `debugComment("ISINDEBUG")`
      fillnull value=0 _time
| bin _time
| stats count by _time
    </query>
    <earliest>$SelectedTime.earliest$</earliest>
    <latest>$SelectedTime.latest$</latest>
  </search>
  <search id="MainSearchStats">
    <query>
      | searchdebug user_search=$form.EnteredSearch|s$ user_earliest=$form.SearchRunEarliest$ user_latest=$form.SearchRunLatest$ $MainSIDObtained$ `debugComment("ISINDEBUG")`
| fields commandCombined
| mvexpand commandCombined
| rex field=commandCombined "^(?&lt;Line&gt;.+)(``RehabSEP``)(?&lt;Command&gt;.+)\2(?&lt;search&gt;.+\|)\2(?&lt;Statement&gt;.+)\2(?&lt;sid&gt;.+)\2(?&lt;Duration&gt;.+)\2(?&lt;ResultCount&gt;(\d+|ERROR))\2(?&lt;PrevCount&gt;\d+)$"
| fields - commandCombined
    </query>
    <earliest>$SelectedTime.earliest$</earliest>
    <latest>$SelectedTime.latest$</latest>
  </search>
  <search id="MainSearchPerformance">
    <query>| rest /services/search/jobs/$SID$ `debugComment("ISINDEBUG")`
| fields performance.command.search.index.duration_secs performance.command.search.rawdata.duration_secs performance.command.search.kv.duration_secs *command.*.input_count *command.*.output_count *command.*.invocations scanCount runDuration</query>
    <earliest>$SelectedTime.earliest$</earliest>
    <latest>$SelectedTime.latest$</latest>
  </search>
  <search id="MainFullSearchPerformance">
    <query>| rest /services/search/jobs/$MainSID$ `debugComment("ISINDEBUG")`
| fields performance.command.search.index.duration_secs performance.command.search.rawdata.duration_secs performance.command.search.kv.duration_secs *command.*.input_count *command.*.output_count *command.*.invocations scanCount runDuration</query>
    <earliest>$SelectedTime.earliest$</earliest>
    <latest>$SelectedTime.latest$</latest>
  </search>
  <search id="MainSearchPerformanceLog">
    <query>| rest /services/search/jobs/$MainSID$/search.log `debugComment("ISINDEBUG")`
| makemv tokenizer="((\d{2,4}\-?){3} (\d{2}\:?){3}\.\d{3} .+)" value
| fields value
    </query>
    <earliest>$SelectedTime.earliest$</earliest>
    <latest>$SelectedTime.latest$</latest>
  </search>
  <search id="MainBreakdownStats">
    <query>
      |loadjob $SID$ |`debugComment("ISINDEBUG")`
 stats count
    </query>
    <earliest>$SelectedTime.earliest$</earliest>
    <latest>$SelectedTime.latest$</latest>
  </search>
  <search id="MainBreakdownTimechart">
    <query>
      |loadjob $SID$ |`debugComment("ISINDEBUG")`
timechart count as "Total Results"
    </query>
    <earliest>$SelectedTime.earliest$</earliest>
    <latest>$SelectedTime.latest$</latest>
  </search>
  <search id="MainFieldSummary">
    <query>$RunSearch$ $SubSIDObtained$ $ShowResultFieldSummary$ fieldsummary `debugComment("ISINDEBUG")`</query>
    <earliest>$SelectedTime.earliest$</earliest>
    <latest>$SelectedTime.latest$</latest>
  </search>
  <search id="MainSearchCommands">
    <query>
      |makeresults | eval enSearch = replace($form.EnteredSearch|s$,"\\\n","") $MainSIDObtained$`debugComment("ISINDEBUG")`
| eval results = commands(if(match(enSearch,"^\s*\|"),enSearch,"search ".enSearch))|fields *
    </query>
    <earliest>$SelectedTime.earliest$</earliest>
    <latest>$SelectedTime.latest$</latest>
  </search>
  <search id="getMacros" base="MainSearchStats">
    <query>
      search Command="Macro*"
    </query>
    <progress>
      <unset token="hasMacros"></unset>
    </progress>
    <done>
      <condition match="$job.resultCount$ &gt; 0">
        <set token="hasMacros"></set>
      </condition>
    </done>
  </search>
  <search id="MainJobStats" base="MainSearchPerformance">
    <query>
      | table scanCount runDuration
      | eval EPS = if(scanCount!=0 and isnotnull(scanCount),scanCount/runDuration,"N/A")
    </query>
  </search>
  <search id="CheckTStats" base="MainSearchCommands">
    <query>| where mvindex(results,0)="search"
| eval statsCmds = mvfilter(match(results, "(timechart|stats|chart)"))
| where mvcount(statsCmds)=1
| eval transCmds = mvfilter(match(statements, "(top|rare|contingency|highlight|addtotals|cofilter|history|mvcombine|xyseries)"))
| search statsCmds = * NOT transCmds = *| fields *</query>
    <progress>
      <unset token="ShowTstats"></unset>
    </progress>
    <done>
      <condition match="$job.resultCount$ &gt; 0">
        <set token="ShowTstats">If your queried fields are extracted at index-time, you may see a significant performance improvement by using tstats</set>
        <set token="ShowReccomendations"></set>
      </condition>
    </done>
  </search>
