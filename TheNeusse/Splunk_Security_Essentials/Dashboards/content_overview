          <query>| eval Status=if(in(datasource,$datasource_selection$), "$SelectedLongName$",Status)
| search Status="$phase$" "Use Case Orig"="$usecase$" "App"="$app$" "Journey"="$journey$" "Data Source"="$datasource$" "Data Source Category"="$datacategory$" "bookmark_status"="$bookmark_status$" "highlight"="$highlight$"
